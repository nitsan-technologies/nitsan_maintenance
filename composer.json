{
  "name": "nitsan/nitsan-maintenance",
  "type": "typo3-cms-extension",
  "description": "Do you want to make your site temporary offline like coming soon and maintenance mode? This extension will help you with simple plug & play to your TYPO3 instance. You can download PRO version with more-features & free-support at https://t3terminal.com/nitsan-maintenance-pro",
  "homepage": "https://extensions.typo3.org/extension/nitsan_maintenance/",
  "license": "GPL-2.0+",
  "keywords": [
    "TYPO3",
    "Sliders",
    "NITSAN"
  ],
  "support": {
    "issues": "https://github.com/nitsan-technologies/nitsan_maintenance/issues"
  },
  "require": {
    "typo3/cms-core": ">=6.2.0"
  },
  "autoload": {
    "psr-4": {
      "Nitsan\\NitsanMaintenance\\": "Classes/"
    }
  },
  "replace": {
      "nitsan_maintenance": "self.version",
      "nitsan/nitsan-maintenance": "self.version"
  },
  "config": {
    "vendor-dir": ".Build/vendor/",
    "bin-dir": ".Build/bin/"
  },
  "extra": {
    "typo3/cms": {
        "cms-package-dir": "{$vendor-dir}/typo3/cms",
        "web-dir": ".Build/public",
        "extension-key": "nitsan_maintenance"
    }
  }
}
